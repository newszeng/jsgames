    "watch:wordle-theme2": "tailwindcss -c ./tailwind.config.cjs -i ./tailwind/wordle-theme2/input.css -o ./admin/static/wordle/theme2/css/main.css --watch",
    "build:wordle-theme3": "tailwindcss -c ./tailwind.config.cjs -i ./tailwind/wordle-theme3/input.css -o ./admin/static/wordle/theme3/css/main.css --minify",
    "watch:wordle-theme3": "tailwindcss -c ./tailwind.config.cjs -i ./tailwind/wordle-theme3/input.css -o ./admin/static/wordle/theme3/css/main.css --watch",
