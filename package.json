    "build:spellsbee.org": "tailwindcss -c ./tailwind.config.cjs -i ./tailwind/wordle-theme1/input.css -o ./static/spellsbee.org/css/main.css --minify",
    "watch:spellsbee.org": "tailwindcss -c ./tailwind.config.cjs -i ./tailwind/wordle-theme1/input.css -o ./static/spellsbee.org/css/main.css --watch"
